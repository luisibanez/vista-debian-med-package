"RTN","RCRJRBD",86,0)
 . . . ;
"RTN","RCRJRBD",87,0)
 . . . S BILLDA=$P($G(^PRCA(433,TRANDA,0)),"^",2)
"RTN","RCRJRBD",88,0)
 . . . I 'BILLDA Q
"RTN","RCRJRBD",89,0)
 . . . S CATEGORY=+$P($G(^PRCA(430,BILLDA,0)),"^",2)
"RTN","RCRJRBD",90,0)
 . . . ;  do not look at prepayments
"RTN","RCRJRBD",91,0)
 . . . I 'CATEGORY!(CATEGORY=26) Q
"RTN","RCRJRBD",92,0)

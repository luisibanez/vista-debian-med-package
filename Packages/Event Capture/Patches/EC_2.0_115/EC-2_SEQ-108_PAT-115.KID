"RTN","EC725U62",43,0)
 .I +Y>0 D
"RTN","EC725U62",44,0)
 ..S COUNT=COUNT+1
"RTN","EC725U62",45,0)
 ..D MES^XPDUTL(" ")
"RTN","EC725U62",46,0)
 ..S STR="   Entry #"_+Y_" for "_$P(Y,U,2)
"RTN","EC725U62",47,0)

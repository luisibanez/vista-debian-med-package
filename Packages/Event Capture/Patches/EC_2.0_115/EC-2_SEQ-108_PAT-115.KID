"RTN","EC725U62",37,0)
 ;
"RTN","EC725U62",38,0)
FILPROC ;File national procedures
"RTN","EC725U62",39,0)
 I '$D(^EC(725,"D",CODE)) D
"RTN","EC725U62",40,0)
 .S ECDINUM=ECDINUM+1,DINUM=ECDINUM,DIC(0)="L",DLAYGO=725,DIC="^EC(725,"
"RTN","EC725U62",41,0)
 .S DIC("DR")="1///^S X=CODE;4///^S X=CPT"
"RTN","EC725U62",42,0)

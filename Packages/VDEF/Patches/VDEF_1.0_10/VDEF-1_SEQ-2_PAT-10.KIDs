 D BMES^XPDUTL("Stopping the MONITOR^VDEFMON task")
"RTN","VDEFP10",45,0)
 N TASK D RTN^%ZTLOAD("VDEFMON","TASK")
"RTN","VDEFP10",46,0)
 S ZTSK=$O(TASK(0)) I ZTSK D
"RTN","VDEFP10",47,0)
 . S X=$$ASKSTOP^%ZTLOAD(ZTSK)
"RTN","VDEFP10",48,0)
 . N I F I=1:1:5 D STAT^%ZTLOAD Q:ZTSK(1)=0!(ZTSK(1)>2)  H 1
"RTN","VDEFP10",49,0)
 . K I D KILL^%ZTLOAD

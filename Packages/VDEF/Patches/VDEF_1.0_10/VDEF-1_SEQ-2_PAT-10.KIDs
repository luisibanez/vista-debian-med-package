"RTN","VDEFP10",13,0)
 ;
"RTN","VDEFP10",14,0)
 ; Suspend running Request Queues
"RTN","VDEFP10",15,0)
 S QUEUE=0 F  S QUEUE=$O(^VDEFHL7(579.3,QUEUE)) Q:'QUEUE  D
"RTN","VDEFP10",16,0)
 . S STAT=$$GET1^DIQ(579.3,QUEUE_",",.09,"I") I STAT="R" D
